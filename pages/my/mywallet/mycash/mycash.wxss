.mycash{border-top: 2rpx solid rgba(229, 229, 229, 1);padding: 80rpx 0 0;text-align: center;position: relative}
.mycash_img{width: 128rpx;height: 128rpx;}
.mycash_name{color: rgba(0, 0, 0, 1);font-size: 32rpx; padding: 30rpx 0 36rpx;}
.mycash_num{color: rgba(0, 0, 0, 1);font-size: 72rpx;font-weight: bold}
.mycash_text{position: fixed;bottom: 144rpx;left: 189rpx;right: 189rpx;}
.mycash_text_1{background-color: rgba(236, 65, 73, 1);border-radius: 16rpx;box-shadow: 0px 5px 5px 0px rgba(236, 65, 73, 0.3);font-weight: bold;color: rgba(255, 255, 255, 1);font-size: 32rpx; padding: 16rpx 0;margin-bottom: 40rpx;border: rgba(236, 65, 73, 1) solid 1px}
.mycash_text_2{color: rgba(236, 65, 73, 1);font-size: 32rpx;background-color: rgba(236, 65, 73, 0);border: rgba(236, 65, 73, 1) solid 1px;border-radius: 16rpx;padding: 16rpx 0;font-weight: bold;}
.mycash_text_3{color: rgba(128, 128, 128, 1);font-size: 28rpx;margin-top: 50rpx;}
.scoll-h{height: 100%}